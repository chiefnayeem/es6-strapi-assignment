{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "p_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "p_caption": {
      "type": "richtext"
    },
    "p_likes": {
      "type": "biginteger",
      "default": "0"
    },
    "p_comments": {
      "collection": "comment",
      "via": "c_post"
    },
    "p_author": {
      "via": "posts",
      "plugin": "users-permissions",
      "model": "user"
    },
    "p_url": {
      "type": "string"
    }
  }
}
